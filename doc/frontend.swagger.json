  "definitions": {
    "v1ErrorObject": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int64",
          "description": "Response error code",
          "pattern": "^[0-9]{1,}$"
        },
        "message": {
          "type": "string",
          "description": "Response error message"
        }
      },
      "title": "错误"
    },
    "v1ErrorsObject": {
      "type": "object",
